<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" 
        android:id="@+id/maini"
        android:orientation="vertical"
        android:padding="5dp"
	    android:layout_height="fill_parent" 
	    android:layout_width="fill_parent">
    
    <LinearLayout
    	android:focusable="true" android:focusableInTouchMode="true"
    	android:layout_width="0px" android:layout_height="0px"/>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:scrollbars="vertical"
    android:layout_height="wrap_content"
    android:id="@+id/tablor"
    android:paddingLeft="4dp"
    android:background="@drawable/shapetable">
  <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:id="@+id/table"
    android:padding="8dp"
    android:paddingTop="0dp"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingBottom="0dp"
    android:background="@drawable/shapeintable">
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" 
        android:id="@+id/maini"
        android:orientation="vertical"
	    android:layout_height="fill_parent" 
	    android:layout_width="wrap_content">

			    <TextView
				    android:id="@+id/freecell"
				    android:layout_width="fill_parent"
				    android:layout_height="wrap_content"
				    android:textStyle="bold"
				    android:text="Allgemein :"
				    >
				</TextView>
				<TextView
				    android:id="@+id/freecell"
				    android:layout_width="fill_parent"
				    android:visibility="invisible"
				    android:layout_height="wrap_content"
				    android:text="GeoTag :"
				    >
				</TextView>
				<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" 
				        android:id="@+id/maini"
				        android:orientation="vertical"
				        android:paddingLeft="5dp"
					    android:layout_height="fill_parent" 
					    android:layout_width="fill_parent">
				<TextView
				    android:id="@+id/freecell"
				    android:layout_width="fill_parent"
				    android:layout_height="wrap_content"
				    android:text="GeoTag :"
				    >
				</TextView>   
				<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" 
				        android:id="@+id/maini"
				        android:gravity="right"
				        android:orientation="horizontal"
					    android:layout_height="fill_parent" 
					    android:layout_width="fill_parent">
					    
				<TextView
				    android:id="@+id/freecell"
				    android:layout_width="fill_parent"
				    android:visibility="invisible"
				    android:layout_weight=".7"
				    android:layout_height="wrap_content"
				    android:text="GeoTag :"
				    >
				</TextView> 
				<ToggleButton
				    android:id="@+id/geotag"
				    android:gravity="right"
				    android:layout_width="fill_parent"
				    android:layout_weight=".3"
				    android:layout_height="wrap_content"
				    android:text="Allgemein :"
				    >
				</ToggleButton>
				    </LinearLayout>
				</LinearLayout>
			        
			        
			        
				<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" 
				        android:id="@+id/maini"
				        android:orientation="vertical"
				        android:paddingLeft="5dp"
					    android:layout_height="fill_parent" 
					    android:layout_width="fill_parent">
				<TextView
				    android:id="@+id/freecell"
				    android:layout_width="fill_parent"
				    android:layout_height="wrap_content"
				    android:text="Zeitstempel :"
				    >
				</TextView>   
				<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" 
				        android:id="@+id/maini"
				        android:gravity="right"
				        android:orientation="horizontal"
					    android:layout_height="fill_parent" 
					    android:layout_width="fill_parent">
					    
				<TextView
				    android:id="@+id/freecell"
				    android:layout_width="fill_parent"
				    android:visibility="invisible"
				    android:layout_weight=".7"
				    android:layout_height="wrap_content"
				    android:text="GeoTag :"
				    >
				</TextView> 
				<Spinner
				    android:id="@+id/zeitstempel"
				    android:gravity="right"
				    android:layout_width="fill_parent"
				    android:layout_weight=".3"
				    android:layout_height="wrap_content"
				    android:text="Allgemein :"
				    >
				</Spinner>
				    </LinearLayout>
				</LinearLayout>    
				    
				    
				
				    
				    
				<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" 
				        android:id="@+id/maini"
				        android:orientation="vertical"
				        android:paddingLeft="5dp"
					    android:layout_height="fill_parent" 
					    android:layout_width="fill_parent">
				<TextView
				    android:id="@+id/freecell"
				    android:layout_width="fill_parent"
				    android:layout_height="wrap_content"
				    android:text="Verlängerungsfaktor :"
				    >
				</TextView>   
				<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" 
				        android:id="@+id/maini"
				        android:gravity="right"
				        android:orientation="horizontal"
					    android:layout_height="fill_parent" 
					    android:layout_width="fill_parent">
					    
				<TextView
				    android:id="@+id/freecell"
				    android:layout_width="fill_parent"
				    android:visibility="invisible"
				    android:layout_weight=".7"
				    android:layout_height="wrap_content"
				    android:text="GeoTag :"
				    >
				</TextView> 
				<Spinner
				    android:id="@+id/verlang"
				    android:gravity="right"
				    android:layout_width="fill_parent"
				    android:layout_weight=".3"
				    android:layout_height="wrap_content"
				    android:text="Allgemein :"
				    >
				</Spinner>
				    </LinearLayout>
				</LinearLayout>
				    
				    
				    
				    
				    
				    
				    
				    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" 
				        android:id="@+id/maini"
				        android:orientation="vertical"
				        android:paddingLeft="5dp"
					    android:layout_height="fill_parent" 
					    android:layout_width="fill_parent">
				<TextView
				    android:id="@+id/freecell"
				    android:layout_width="fill_parent"
				    android:layout_height="wrap_content"
				    android:text="Blendenstufen :"
				    >
				</TextView>   
				<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" 
				        android:id="@+id/maini"
				        android:gravity="right"
				        android:orientation="horizontal"
					    android:layout_height="fill_parent" 
					    android:layout_width="fill_parent">
					    
				<TextView
				    android:id="@+id/freecell"
				    android:layout_width="fill_parent"
				    android:visibility="invisible"
				    android:layout_weight=".7"
				    android:layout_height="wrap_content"
				    android:text="GeoTag :"
				    >
				</TextView> 
				<Spinner
				    android:id="@+id/blenden"
				    android:gravity="right"
				    android:layout_width="fill_parent"
				    android:layout_weight=".3"
				    android:layout_height="wrap_content"
				    android:text="Allgemein :"
				    >
				</Spinner>
				    </LinearLayout>
				</LinearLayout>

				    <LinearLayout
				        xmlns:android="http://schemas.android.com/apk/res/android"
				        android:id="@+id/LinearLayout01"
				        android:layout_width="fill_parent"
				        android:layout_height="fill_parent"
				        android:orientation="vertical"
				        android:paddingLeft="5dp" >

				        <TextView
				            android:id="@+id/TextView01"
				            android:layout_width="fill_parent"
				            android:layout_height="wrap_content"
				            android:text="Tutorial zurücksetzen :" />

				        <LinearLayout
				            xmlns:android="http://schemas.android.com/apk/res/android"
				            android:id="@+id/LinearLayout01"
				            android:layout_width="fill_parent"
				            android:layout_height="fill_parent"
				            android:gravity="right"
				            android:orientation="horizontal" >

				            <TextView
				                android:id="@+id/TextView01"
				                android:layout_width="fill_parent"
				                android:layout_height="wrap_content"
				                android:layout_weight=".7"
				                android:text="GeoTag :"
				                android:visibility="invisible" />

				            <Button
				                android:id="@+id/ButtonTutorialBack"
				                android:layout_width="fill_parent"
				                android:layout_height="wrap_content"
				                android:layout_weight=".3"
				                android:gravity="center"
				                android:text="Hilfe zurücksetzen" />
				        </LinearLayout>
				    </LinearLayout>
				        
				        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" 
				        android:id="@+id/maini"
				        android:orientation="vertical"
				        android:paddingLeft="5dp"
					    android:layout_height="fill_parent" 
					    android:layout_width="fill_parent">
				<TextView
				    android:id="@+id/freecell"
				    android:layout_width="fill_parent"
				    android:layout_height="wrap_content"
				    android:text="Einstellungen auf einer Seite anzeigen :"
				    >
				</TextView>   
				<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" 
				        android:id="@+id/maini"
				        android:gravity="right"
				        android:orientation="horizontal"
					    android:layout_height="fill_parent" 
					    android:layout_width="fill_parent">
					    
				<TextView
				    android:id="@+id/freecell"
				    android:layout_width="fill_parent"
				    android:visibility="invisible"
				    android:layout_weight=".7"
				    android:layout_height="wrap_content"
				    android:text="GeoTag :"
				    >
				</TextView> 
				<ToggleButton
				    android:id="@+id/settings"
				    android:gravity="right"
				    android:layout_width="fill_parent"
				    android:layout_weight=".3"
				    android:layout_height="wrap_content"
				    >
				</ToggleButton>
				    </LinearLayout>
				</LinearLayout>
				    
				    
				    
				    
				    
				    
			        
			        
      </LinearLayout>
   </TableLayout>    
         
</TableLayout>
</LinearLayout>