<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/maini" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:orientation="vertical"
	android:padding="5dp">

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <TableLayout
                android:id="@+id/TableLayout02"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="4dp"
                android:scrollbars="vertical" >

                <TableLayout
                    android:id="@+id/TableLayout03"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:padding="8dp"
                    android:paddingBottom="0dp"
                    android:paddingLeft="0dp"
                    android:paddingRight="0dp"
                    android:paddingTop="0dp" >

                    <TableLayout
                        android:id="@+id/TableLayout04"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content" >

                        <TextView
                            android:id="@+id/text_messung"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingLeft="12dp"
                            android:text="@string/measurement" />

                        <LinearLayout
                            android:id="@+id/mainiof"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight=".44"
                            android:gravity="left" >

                            <TextView
                                android:id="@+id/TextView08"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight=".34"
                                android:visibility="invisible" />

                            <Spinner
                                android:id="@+id/spinner_messmethode"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight=".1" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/text_belichtungskorrektur"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingLeft="12dp"
                            android:text="@string/exposure_correction" />

                        <LinearLayout
                            android:id="@+id/LinearLayout02"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight=".44"
                            android:gravity="left" >

                            <TextView
                                android:id="@+id/TextView10"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight=".34"
                                android:visibility="invisible" />

                            <Spinner
                                android:id="@+id/spinner_belichtungs_korrektur"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight=".1" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/text_makro_vf"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight=".34"
                            android:paddingLeft="12dp"
                            android:text="@string/macro_vf" />

                        <LinearLayout
                            android:id="@+id/LinearLayout01"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight=".44"
                            android:gravity="left" >

                            <TextView
                                android:id="@+id/TextView09"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight=".34"
                                android:visibility="invisible" />

                            <Spinner
                                android:id="@+id/spinner_makro_vf"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight=".1" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/text_blitz_korrektur"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingLeft="12dp"
                            android:text="@string/filter_vf" />

                        <LinearLayout
                            android:id="@+id/LinearLayout03"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight=".44"
                            android:gravity="left" >

                            <TextView
                                android:id="@+id/TextView11"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight=".34"
                                android:visibility="invisible" />

                            <Spinner
                                android:id="@+id/spinner_filter_vf"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight=".1" />
                        </LinearLayout>
                    </TableLayout>
                </TableLayout>
            </TableLayout>

            <TableLayout
                android:id="@+id/TableLayout02"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="4dp"
                android:scrollbars="vertical" >

                <TableLayout
                    android:id="@+id/TableLayout03"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:padding="8dp"
                    android:paddingBottom="0dp"
                    android:paddingLeft="0dp"
                    android:paddingRight="0dp"
                    android:paddingTop="0dp" >

                    <TableLayout
                        android:id="@+id/TableLayout04"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >

                        <TextView
                            android:id="@+id/text_blitz"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingLeft="12dp"
                            android:text="@string/flash" />

                        <LinearLayout
                            android:id="@+id/LinearLayout04"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight=".44"
                            android:gravity="left" >

                            <TextView
                                android:id="@+id/TextView12"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight=".34"
                                android:visibility="invisible" />

                            <Spinner
                                android:id="@+id/spinner_blitz"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight=".1" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/text_blitz_korrektur"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingLeft="12dp"
                            android:text="@string/flash_correction" />

                        <LinearLayout
                            android:id="@+id/LinearLayout03"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight=".44"
                            android:gravity="left" >

                            <TextView
                                android:id="@+id/TextView11"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight=".34"
                                android:visibility="invisible" />

                            <Spinner
                                android:id="@+id/spinner_blitz_korrektur"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight=".1" />
                        </LinearLayout>
                    </TableLayout>
                </TableLayout>
            </TableLayout>
        </LinearLayout>

    </ScrollView>











</LinearLayout>