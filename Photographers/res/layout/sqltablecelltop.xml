<LinearLayout 
    android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:layout_weight="1"
	>
	<TextView 
	    android:id="@+id/timecell"
	    android:layout_width="fill_parent"
	    android:gravity="center"
	    android:layout_weight=".1"
	    android:layout_height="wrap_content"
	    android:text="12:30 "
	    >
	</TextView>
	<TextView 
	    android:id="@+id/gesamtcell"
	    android:layout_width="fill_parent"
	    android:gravity="center"
	    android:layout_weight=".1"
	    android:layout_height="wrap_content"
	    android:text="1 Std "
	    >
	</TextView>
	<TextView 
	    android:id="@+id/pausecell"
	    android:layout_width="fill_parent"
	    android:gravity="center"
	    android:layout_weight=".1"
	    android:layout_height="wrap_content"
	    android:text="0 Min "
	    >
	</TextView>
</LinearLayout>

    
    
</LinearLayout>